<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <SolidColorBrush x:Key="ListViewBackgroundBrush" Color="#FF5B5B5B" />
    <SolidColorBrush x:Key="ListViewHeaderBrush" Color="Black" />
    <SolidColorBrush x:Key="ListViewItemBrush" Color="Black" />
    <SolidColorBrush x:Key="ListViewItemHoverBrush" Color="#FF5A5A5A" />
    <SolidColorBrush x:Key="ListViewItemSelectedBrush" Color="#FF5F5F5F" />

    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="#FF3B3B3B" />
    <SolidColorBrush x:Key="TextBoxBackgroundBrush" Color="#FF4B4B4B" />
    
    <Color x:Key="ShadowColor">Silver</Color>
    <Color x:Key="ButtonNormalColor">#FF5B5B5B</Color>
    <Color x:Key="DisabledButtonColor">#FF6B6B6B</Color>
    <Color x:Key="DisabledForegroundColor">Silver</Color>
    <Color x:Key="ButtonMouseOverColor">#FF4B4B4B</Color>
    <Color x:Key="ButtonPressedColor">#FF3B3B3B</Color>

</ResourceDictionary>